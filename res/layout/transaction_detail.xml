<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

    <TextView
        android:id="@+id/CheckNo"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|center_horizontal"
        android:gravity="center_vertical"
        android:text="CheckNo"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/map_various" />

    <TextView
        android:id="@+id/MemberId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MemberId"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/BusinessDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BusinessDate"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/EndTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EndTime"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/OutletCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OutletCode"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/NetSale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NetSale"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/ServiceCharge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ServiceCharge"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Tax"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tax"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/ActualPaid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ActualPaid"
        android:textSize="18sp" />
    
    <TextView
        android:id="@+id/BeginDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="18sp" />

    <TextView
        android:id="@+id/BeginTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="18sp" />

    <TextView
        android:id="@+id/EndDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="18sp" />

</LinearLayout>